<h2>Wikinimous</h2>
<p><%= link_to "New article", new_article_path%></p>

<h1>Editing article</h1>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit  %>
<% end %>

<p><%= link_to "Back", articles_path %> | <%= link_to "Show", article_path %></p>
